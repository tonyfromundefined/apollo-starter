{
  "name": "apollo-starter",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "predev": "rimraf ./dist",
    "dev": "dotenv -e .env.development -- webpack --config webpack.config.dev.js --info-verbosity none",
    "debug": "dotenv -e .env.development -- ts-node-dev --respawn --transpileOnly --inspect ./src/index.ts",
    "generate": "graphql-codegen --config codegen.yml",
    "prebuild": "yarn generate && rimraf ./dist",
    "build": "webpack --config webpack.config.prod.js",
    "start": "dotenv -e .env.production -- node ./dist/index.js",
    "lint": "tslint --project .",
    "typeorm:migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "typeorm:migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run"
  },
  "dependencies": {
    "apollo-server-express": "^2.8.2",
    "cors": "^2.8.5",
    "dotenv-cli": "^2.0.1",
    "es6-promisify": "^6.0.1",
    "express": "^4.17.1",
    "git-last-commit": "^1.0.0",
    "glob": "^7.1.4",
    "graphql": "^14.4.2",
    "graphql-import": "^0.7.1",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.6.1",
    "@graphql-codegen/schema-ast": "^1.6.1",
    "@graphql-codegen/typescript": "1.6.1",
    "@graphql-codegen/typescript-resolvers": "1.6.1",
    "@types/es6-promisify": "^6.0.0",
    "@types/express": "^4.17.1",
    "@types/graphql": "^14.2.3",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "graphql-import-webpack-loader": "^1.0.0",
    "nodemon-webpack-plugin": "^4.0.8",
    "ts-loader": "^6.0.4",
    "ts-node-dev": "^1.0.0-pre.41",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^4.0.0"
  }
}
